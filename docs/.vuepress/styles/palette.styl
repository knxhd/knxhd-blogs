// 蓝色主题色
$accentColor = #42B883

.dark .content__default code {
  background-color: rgba(58,58,92,0.7);
  color: #fff;
}
blockquote {
  border-left: 4px solid #42b983;
  padding: 10px 15px;
  color: #777;
  background-color: rgba(66, 185, 131, .1);
}
/** focus mode */
.on-focus-mode blockquote {
  border-left-color: rgba(85, 85, 85, 0.12);
}